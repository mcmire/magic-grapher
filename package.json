{
  "name": "magic-grapher",
  "version": "0.0.0",
  "description": "Generate graphs, easy!",
  "main": "index.js",
  "scripts": {
    "start": "parcel serve --log-level 4 src/index.html",
    "test": "cypress run",
    "cy": "cypress open",
    "build": "rm -rf autogenerated dist && mkdir autogenerated dist && parcel build --log-level 4 src/index.html && touch dist/favicon.ico",
    "ci:start": "serve --no-clipboard --listen ${PORT:-1234} dist",
    "ci": "run-p --race ci:start test"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/mcmire/treemagic"
  },
  "author": "Elliot Winkler <elliot.winkler@gmail.com>",
  "license": "MIT",
  "devDependencies": {
    "cypress": "^3.6.1",
    "elm-format": "^0.8.2",
    "elm-hot": "^1.1.2",
    "elm-json": "^0.2.2",
    "node-elm-compiler": "^5.0.4",
    "parcel": "^1.12.4",
    "prettier": "^1.18.2"
  },
  "dependencies": {
    "normalize.css": "^8.0.1",
    "npm-run-all": "^4.1.5",
    "reset.css": "^2.0.2",
    "serve": "^11.2.0"
  }
}
